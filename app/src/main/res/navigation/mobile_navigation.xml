<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.farmgame.frontEnd.main.LandFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_land" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.farmgame.frontEnd.main.storeFragment"
        android:label="Buy"
        tools:layout="@layout/fragment_store" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.farmgame.frontEnd.main.SellFragment"
        android:label="Sell"
        tools:layout="@layout/fragment_sell" />

    <fragment
        android:id="@+id/navigation_warehouse"
        android:name="com.farmgame.frontEnd.main.WarehouseFragment"
        android:label="Warehouse"
        tools:layout="@layout/fragment_warehouse" />
</navigation>